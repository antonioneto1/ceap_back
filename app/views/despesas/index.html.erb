<p id="notice"><%= notice %></p>

<h1>Despesas</h1>

<table>
  <thead>
    <tr>
      <th>Deputado</th>
      <th>Foranecedor</th>
      <th>Cnpjcpf</th>
      <th>Numero</th>
      <th>Tipodocumento</th>
      <th>Dataemissao</th>
      <th>Valordocumento</th>
      <th>Valorglosa</th>
      <th>Valorliquido</th>
      <th>Mes</th>
      <th>Ano</th>
      <th>Ressarcimento</th>
      <th>Restituicao</th>
      <th>Iddocumento</th>
      <th>Urldocumento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @despesas.each do |despesa| %>
      <tr>
        <td><%= despesa.deputado_id %></td>
        <td><%= despesa.foranecedor %></td>
        <td><%= despesa.cnpjCPF %></td>
        <td><%= despesa.numero %></td>
        <td><%= despesa.tipoDocumento %></td>
        <td><%= despesa.dataEmissao %></td>
        <td><%= despesa.valorDocumento %></td>
        <td><%= despesa.valorGlosa %></td>
        <td><%= despesa.valorLiquido %></td>
        <td><%= despesa.mes %></td>
        <td><%= despesa.ano %></td>
        <td><%= despesa.ressarcimento %></td>
        <td><%= despesa.restituicao %></td>
        <td><%= despesa.idDocumento %></td>
        <td><%= despesa.urlDocumento %></td>
        <td><%= link_to 'Show', despesa %></td>
        <td><%= link_to 'Edit', edit_despesa_path(despesa) %></td>
        <td><%= link_to 'Destroy', despesa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Despesa', new_despesa_path %>
