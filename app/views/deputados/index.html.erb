<p id="notice"><%= notice %></p>

<h1>Deputados</h1>

<table>
  <thead>
    <tr>
      <th>Nomeparlamentar</th>
      <th>Cpf</th>
      <th>Iddeputado</th>
      <th>Numerocarteiraparlamentar</th>
      <th>Legislatura</th>
      <th>Siglauf</th>
      <th>Siglapartido</th>
      <th>Codigolegislatura</th>
      <th>Numerodeputadoid</th>
      <th>Urlphoto</th>
      <th>Despesas ids</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deputados.each do |deputado| %>
      <tr>
        <td><%= deputado.nomeParlamentar %></td>
        <td><%= deputado.cpf %></td>
        <td><%= deputado.idDeputado %></td>
        <td><%= deputado.numeroCarteiraParlamentar %></td>
        <td><%= deputado.legislatura %></td>
        <td><%= deputado.siglaUF %></td>
        <td><%= deputado.siglaPartido %></td>
        <td><%= deputado.codigoLegislatura %></td>
        <td><%= deputado.numeroDeputadoID %></td>
        <td><%= deputado.urlPhoto %></td>
        <td><%= deputado.despesas_ids %></td>
        <td><%= link_to 'Show', deputado %></td>
        <td><%= link_to 'Edit', edit_deputado_path(deputado) %></td>
        <td><%= link_to 'Destroy', deputado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deputado', new_deputado_path %>
