<p id="notice"><%= notice %></p>

<h1>Deputados</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Id. Deputado</th>
      <th>N.Carteira</th>
      <th>UF</th>
      <th>Sigla</th>
      <th>N Deputado</th>
      <th>Total de despesas</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deputados.each do |deputado| %>
      <tr>
        <td><%= deputado.nomeParlamentar %></td>
        <td><%= deputado.cpf %></td>
        <td><%= deputado.idDeputado %></td>
        <td><%= deputado.numeroCarteiraParlamentar %></td>
        <td><%= deputado.siglaUF %></td>
        <td><%= deputado.siglaPartido %></td>
        <td><%= deputado.numeroDeputadoID %></td>
        <td><%= deputado.total_despesas %></td>
        <td><%= link_to 'Show', deputado %></td>
        <td><%= link_to 'Edit', edit_deputado_path(deputado) %></td>
        <td><%= link_to 'Destroy', deputado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deputado', new_deputado_path %>
